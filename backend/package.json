{
  "name": "aerospace-platform-backend",
  "version": "1.0.0",
  "description": "航空器件管理平台后端API服务",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "db-server": "node database-server.js",
    "db-setup": "python scripts/setup_database.py",
    "db-setup-mongo": "python scripts/setup_mongodb.py",
    "db-import": "python scripts/import_doeeet_data.py",
    "db-import-test": "python scripts/import_doeeet_data.py --test",
    "test:cache": "ts-node src/scripts/testCache.ts",
    "cache:warmup": "ts-node src/scripts/warmupCache.ts",
    "cache:clear": "ts-node src/scripts/clearCache.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "aerospace",
    "components",
    "api",
    "nodejs",
    "express",
    "mongodb",
    "typescript"
  ],
  "author": "Aerospace Platform Team",
  "license": "MIT",
  "dependencies": {
    "@types/ioredis": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "helmet": "^6.1.5",
    "ioredis": "^5.8.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^6.12.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^27.5.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^14.18.63",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.0",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.5",
    "ts-node-dev": "^1.1.8",
    "tsx": "^4.20.6",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
}
